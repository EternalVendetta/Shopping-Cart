  setCartItems(cartItems) \n    }\n  }\n\n  const removeFromCart = product => {\n\n  }\n\n  // JSX\n  return (\n    <div className='grid-container'>\n      <Header />\n      <main>\n        <div className='content'>\n          <div className='products-container'>\n            <Filter \n              size={size}\n              sort={sort}\n              count={products.length}\n              handleSize={handleSize}\n              handleSort={handleSort}\n            />\n            <Products \n              products={products} \n              handleAddToCart={handleAddToCart}  \n            />\n          </div>\n          <div className='sidebar'>\n            <Cart\n              cartItems={cartItems}\n              removeFromCart={removeFromCart}\n            />\n          </div>\n        </div>\n      </main>\n        {/* Footer goes here */}\n    </div>\n  );\n}\n\nexport default App;\n","C:\\Users\\etern\\OneDrive\\Bureau\\Development Web\\Codecademy Projects\\shopping-cart\\src\\reportWebVitals.js",[],"C:\\Users\\etern\\OneDrive\\Bureau\\Development Web\\Codecademy Projects\\shopping-cart\\src\\components\\Products.jsx",[],"C:\\Users\\etern\\OneDrive\\Bureau\\Development Web\\Codecademy Projects\\shopping-cart\\src\\components\\Filter.jsx",[],"C:\\Users\\etern\\OneDrive\\Bureau\\Development Web\\Codecademy Projects\\shopping-cart\\src\\components\\Cart.jsx",[],"C:\\Users\\etern\\OneDrive\\Bureau\\Development Web\\Codecademy Projects\\shopping-cart\\src\\components\\Header.jsx",[],{"ruleId":"57","severity":1,"message":"58","line":12,"column":20,"nodeType":"59","messageId":"60","endLine":12,"endColumn":31},{"ruleId":"57","severity":1,"message":"61","line":13,"column":10,"nodeType":"59","messageId":"60","endLine":13,"endColumn":14},{"ruleId":"57","severity":1,"message":"62","line":13,"column":16,"nodeType":"59","messageId":"60","endLine":13,"endColumn":23},{"ruleId":"57","severity":1,"message":"63","line":14,"column":10,"nodeType":"59","messageId":"60","endLine":1